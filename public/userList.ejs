            <div class="Value"><%=myArr[i].gender%></div>
        </div>
            <div class="object">
            <div class="Heading">User Id: </div>
            <div class="Value"><%=myArr[i].userId%></div>
        </div>
            <div class="object">
            <div class="Heading">Balance: </div>
            <div class="Value">â‚¹ <%=myArr[i].balanceAmount%></div>
        </div>
            <div class="object">
            <div class="Heading">Date: </div>
            <div class="Value"><%=myArr[i].date%></div>
            </div>            
        </div>
        
        <% }%>
    </div>
    </div>
