            <div class="rowdies-bold"><u>User Records!</u></div>
            <img src="./Styling/img4.png" alt="logo-image" >
        </div>
        <div class="contentPart">
    <% for(var i=myArr.length-1;i>=0;i--) {%>
        
        <div class="divContent">
            <div class="headerColor object"></div>
            <div class="object">
            <div class="Heading">User Name: </div>
            <div class="Value"><%=myArr[i].userName%></div>
        </div>
